{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "headers": [
    {"source": "/service-worker.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}]}
  ]
  ,
  "storage": {
    "rules": "storage.rules"
  }
}
